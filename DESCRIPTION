Package: dfdiff
Type: Package
Title: Compares dataframes
Version: 0.1.0
Authors@R: c( 
    person("Craig" ,"Gower" , email = "craig.gower@roche.com" , role = c("cre","aut")),
    person("Kieran", "Martin" , email = "kieran.martin@roche.com" , role = "aut")
